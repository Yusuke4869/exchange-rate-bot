{
  "name": "exchange-rate-bot",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "Yusuke4869",
  "scripts": {
    "dev": "ts-node-dev src/main.ts",
    "dev:commands": "ts-node-dev src/deployCommands.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "start-exchange-rate-bot": "node dist/main.js",
    "deploy:commands": "node dist/deployCommands.js",
    "lint": "eslint --ignore-path .gitignore src/**/*.ts",
    "lint:fix": "eslint --ignore-path .gitignore src/**/*.ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "yarn lint:fix",
      "yarn format"
    ]
  },
  "packageManager": "yarn@4.10.3",
  "volta": {
    "node": "24.11.0"
  },
  "dependencies": {
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.18.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
